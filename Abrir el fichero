#include <stdio.h>
#include <stdlib.h>

int main() {
   FILE *archivo;
   char linea[100]; // Buffer para almacenar cada línea del archivo

   archivo = fopen("generacion_por_tecnologias_21_222_puntos_simplificado.csv", "r"); // Abre el archivo en modo lectura

   if (archivo == NULL) {
      printf("No se pudo abrir el archivo.\n");
      return 1;
   }

   // Leer y procesar cada línea del archivo
   while (fgets(linea, sizeof(linea), archivo)) {
      // Realizar las operaciones necesarias con cada línea
      printf("%s", linea);
   }

   fclose(archivo); // Cierra el archivo

   return 0;
}

